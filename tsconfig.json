{
  "compilerOptions": {
    "target": "es6",
    "module": "CommonJS",
    "moduleResolution": "node",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "resolveJsonModule": true,
    "esModuleInterop": true,
  },
  "include": ["src"],
  "exclude": ["./node_modules/**/*"]
}
